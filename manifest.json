{
  "name": "LD Engine",
  "version": "13.2.29",
  "description": "Imagine a world where you didn't hate your email.",
  "permissions": [
  ],
  "manifest_version": 2,
  "icons": {
    "48" : "sample-48.png",
    "128" : "sample-128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://mail.google.com/*"],
      "js" : ["async.min.js","jquery-1.8.2.min.js","underscore-min.js","date.js","jquery.dotdotdot-1.5.1.js","jsrender.js","jquery.views.js","jquery.observable.js","ldengine.js", "moment.min.js", "jquery-ui-1.10.1.custom.min.js"],
      "css" : ["ldengine.css", "hint.min.css", "jquery-ui-1.10.1.custom.css"]
    }
  ],
  "permissions": [
    "http://apps.ldengine.com/",
    "https://apps.ldengine.com/",
    "http://preview.engine.co/",
    "https://preview.engine.co/",
    "http://apps.engine.co/",
    "https://apps.engine.co/",
    "http://127.0.0.1:3000/",
    "storage"
  ],
  "web_accessible_resources": [
    "ldengine.tmpl",
    "snippet.tmpl",
    "popup.tmpl",
    "progressbar.tmpl",
    "unauthenticated.tmpl",
    "senderInfo.tmpl",
    "bottomBar.tmpl",
    "noSnippets.tmpl",
    "ajax-loader.gif",
    "icon_close.gif",
    "close_message.gif",
    "image_anonymousLarge@2x.png",
    "logo_engine@2x.png",
    "logo_engine@2x_dark.png",
	"search_glass.png",
	"gmail.png",
	"facebook.png",
	"Twitter.png",
	"logo_engine_dark.png"
  ],
  "options_page": "options.html",
  "browser_action": { 
	  "default_icon": {
		  "19" : "logo_engine_dark.png",
		  "38" : "logo_engine_dark.png"
		},
	  "default_title": "Engine Inc."
	}
}
