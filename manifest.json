{
  "name": "LD Engine",
  "version": "13.2.15",
  "description": "Imagine a world where you didn't hate your email.",
  "permissions": [
  ],
  "manifest_version": 2,
  "icons": {
    "48" : "sample-48.png",
    "128" : "sample-128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://mail.google.com/*"],
      "js" : ["async.min.js","jquery-1.8.2.min.js","underscore-min.js","date.js","jquery.dotdotdot-1.5.1.js","jsrender.js","jquery.views.js","jquery.observable.js","ldengine.js", "moment.min.js"],
      "css" : ["ldengine.css", "hint.min.css"]
    }
  ],
  "permissions": [
    "http://apps.ldengine.com/",
    "https://apps.ldengine.com/",
    "http://preview.engine.co/",
    "https://preview.engine.co/",
    "http://apps.engine.co/",
    "https://apps.engine.co/",
    "http://127.0.0.1:3000/",
    "storage"
  ],
  "web_accessible_resources": [
    "ldengine.tmpl",
    "snippet.tmpl",
    "popup.tmpl",
    "progressbar.tmpl",
    "unauthenticated.tmpl",
    "senderInfo.tmpl",
    "noResponse.tmpl",
    "noSnippets.tmpl",
    "ajax-loader.gif",
    "icon_close.gif",
    "close_message.gif",
    "image_anonymousLarge@2x.png",
    "logo_engine@2x.png",
	"search_glass.png"
  ],
  "options_page": "options.html"
}
